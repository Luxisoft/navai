{
  "name": "@navai/voice-mobile",
  "version": "0.1.1",
  "description": "Mobile helpers to integrate OpenAI Realtime voice with Navai backend routes",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "navai-generate-mobile-loaders": "./bin/generate-mobile-module-loaders.mjs",
    "navai-setup-voice-mobile": "./bin/auto-configure-consumer.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "react-native": "./dist/index.cjs",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "generate:module-loaders": "node ./bin/generate-mobile-module-loaders.mjs",
    "postinstall": "node ./bin/auto-configure-consumer.mjs",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint": "tsc --noEmit -p tsconfig.json"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-native": ">=0.72",
    "react-native-webrtc": ">=124"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
